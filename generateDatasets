# Command to downsample the VLP64 files (no ground labels) 
python scripts/convert.py \
--inpath ./data/squeezed/no_ground/ng_vlp64_v1/ \
--outpath ./data/squeezed/no_ground/ \
--conv downs

# Command to convert the VLP64 .npy files (without ground labels) to .txt files 
python scripts/convert.py \
--inpath ./data/squeezed/no_ground/ng_v1_vlp64/ \
--outpath ./data/ground/ \
--conv txt

# Command to annotate VLP64 .txt files
./extractGround --inpath ../data/ground/ng_vlp64_v1/ --outpath ../data/ground/g_vlp64_v --seg 3 --lpr 20

# Command to convert the VLP64 .txt files (with ground labels) to .npy
python scripts/convert.py \
--inpath ./data/ground/g_vlp64_v1/ \
--outpath ./data/squeezed/ground/ \
--outdir vlp64 \
--conv npy

# Command to downsample the VLP64 files (with ground labels) 
python scripts/convert.py \
--inpath ./data/squeezed/ground/n_vlp64_v1/ \
--outpath ./data/squeezed/ground/ \
--conv downs

